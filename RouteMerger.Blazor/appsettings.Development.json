{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5432"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "ConnectionStrings": {
    "RouteMergerDbContext": "Host=routemerger.postgres;Port=5432;Database=routemergerdb;Username=postgres;Password=postgres",
    "RouteMergerDbContextLocal": "Host=localhost;Port=5431;Database=routemergerdb;Username=postgres;Password=postgres"
  }
}
